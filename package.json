{
  "name": "dog-api-tests",
  "version": "1.0.0",
  "description": "Testes automatizados de API para Dog API usando Playwright e TypeScript",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:docker": "docker-compose up --build",
    "test:docker:clean": "docker-compose down -v",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\""
  },
  "keywords": [
    "playwright",
    "api-testing",
    "typescript",
    "dog-api",
    "qa"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "allure-playwright": "^2.10.0",
    "eslint": "^8.54.0",
    "playwright-ctrf-json-reporter": "^0.0.26",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  }
}

